<Window x:Name="window_dialog" x:Class="GestCommerciale.PL.Toast_Notifications.Dialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GestCommerciale.PL.Toast_Notifications"
        mc:Ignorable="d"
        Title="Dialog" Height="155" Width="455" WindowStartupLocation="CenterScreen" WindowStyle="None" Background="#FFF1F1F1" Closing="window_dialog_Closing">
    <Grid Margin="0,0,0,0">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="55*"/>
            <ColumnDefinition Width="36*"/>
        </Grid.ColumnDefinitions>
        <Image HorizontalAlignment="Left" Width="130" Height="134" Source="/PL/Toast_Notifications/icons8-consumption-96.png" Margin="0,3,0,2"/>
        <Label x:Name="label_msg" Content="Message" HorizontalAlignment="Left" VerticalContentAlignment="Center" Margin="130,0,0,0" Width="325" HorizontalContentAlignment="Center" FontSize="14" Grid.ColumnSpan="2"/>


    </Grid>
</Window>
